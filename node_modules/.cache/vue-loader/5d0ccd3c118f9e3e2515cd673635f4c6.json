        </v-flex>\n\n                                    <v-flex>\n                                        <v-select\n                                            v-model=\"city\"\n                                            :items=\"cities\"\n                                            menu-props=\"auto\"\n                                            label=\"Seleccionar ciudad\"\n                                            hide-details\n                                            prepend-icon=\"map\"\n                                            single-line\n                                        ></v-select>\n                                    </v-flex>\n\n                                    <v-flex>\n                                        <v-text-field :color=\"color_base\"\n                                            \n                                            label=\"Correo electrónico\"\n                                            id=\"email\"\n                                            v-model=\"username\"\n                                        ></v-text-field>\n                                    </v-flex>\n\n                                    <v-flex>\n                                        <v-text-field :color=\"color_base\"\n                                            :append-icon=\"show ? 'visibility' : 'visibility_off'\"\n                                            :rules=\"[rules.required]\"\n                                            :type=\"show ? 'text' : 'password'\"\n                                            \n                                            label=\"Contraseña\"\n                                            @click:append=\"show = !show\"\n                                            id=\"rpass\"\n                                            v-model=\"password\"\n                                        ></v-text-field>\n                                    </v-flex>\n                                    <v-flex>\n                  